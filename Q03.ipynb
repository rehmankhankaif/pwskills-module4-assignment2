   "source": [
    "Write a code to check if a given string is a palindrome or not"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Your name is a palindrome\n"
     ]
    }
   ],
   "source": [
    "name = input(\"Please enter your name: \")\n",
    "name = name.lower()\n",
    "\n",
    "if name == name[::-1]:\n",
    "    print(\"Your name is a palindrome\")\n",
    "else:\n",
    "    print(\"Your name is not a palindrome\")\n"
   ]
  }
